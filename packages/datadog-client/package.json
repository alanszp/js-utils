{
  "name": "@alanszp/datadog-client",
  "version": "1.0.1",
  "description": "alanszp's client for datadog. It's just a wrapper :D",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "license": "MIT",
  "files": [
    "**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/nyc mocha",
    "compile": "rm -rf ./dist && ./node_modules/.bin/tsc",
    "compile-watch": "./node_modules/.bin/tsc -w",
    "build": "yarn run compile",
    "prepack": "yarn run build",
    "postpack": "rm -rf ./dist"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "datadog-metrics": "^0.8.1"
  },
  "devDependencies": {
    "@types/datadog-metrics": "^0.6.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^15.12.3",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^5.11.0",
    "typescript": "^4.3.4"
  }
}
