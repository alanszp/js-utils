{
  "name": "@alanszp/datadog-client",
  "version": "1.0.2",
  "description": "Alan's client for datadog. It's just a wrapper :D",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "rm -rf ./dist && ./node_modules/.bin/tsc --declaration",
    "compile-watch": "./node_modules/.bin/tsc -w",
    "build": "yarn run compile",
    "prepack": "yarn run build"
  },
  "dependencies": {
    "datadog-metrics": "^0.8.1"
  },
  "devDependencies": {
    "@types/datadog-metrics": "^0.6.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^15.12.3",
    "ts-node": "^10.0.0",
    "tslint": "^5.11.0",
    "typescript": "^4.3.4"
  }
}
