{
  "name": "@alanszp/core",
  "version": "1.3.1",
  "description": "Alan's core TS/JS lib.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "**/*"
  ],
  "publishConfig": {
